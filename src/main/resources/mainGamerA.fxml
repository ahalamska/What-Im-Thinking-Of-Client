<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="333.0" prefWidth="569.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.MainGamerA">
   <Pane fx:id="waitingPane" layoutX="-1.0" prefHeight="341.0" prefWidth="601.0" style="-fx-background-color: #030303;">
      <Text fill="#e5e5e5" layoutX="104.0" layoutY="157.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wait for Question...">
         <font>
            <Font name="Calibri" size="49.0" />
         </font>
      </Text>
   </Pane>
   <Pane fx:id="questionPane" layoutX="-1.0" prefHeight="341.0" prefWidth="601.0" style="-fx-background-color: #030303;" visible="false">
      <TextField fx:id="answer" layoutX="159.0" layoutY="155.0" prefHeight="85.0" prefWidth="284.0" promptText="name" style="-fx-background-color: #353535; -fx-text-fill: white;" text="answer">
         <font>
            <Font name="Calibri" size="41.0" />
         </font>
      </TextField>
      <Button fx:id="ok" layoutX="251.0" layoutY="279.0" mnemonicParsing="false" onAction="#sendAnswer" prefHeight="31.0" prefWidth="101.0" style="-fx-background-color: #353535;" text="OK" textFill="#d0cece">
         <font>
            <Font name="Calibri" size="19.0" />
         </font>
      </Button>
      <Label fx:id="questionLabel" layoutX="52.0" layoutY="27.0" prefHeight="78.0" prefWidth="497.0" style="-fx-text-fill: white;" text="QUESTION">
         <font>
            <Font name="Calibri" size="22.0" />
         </font>
      </Label>
   </Pane>
   <Button layoutX="573.0" mnemonicParsing="false" onAction="#exitToMainMenu" prefHeight="64.0" prefWidth="55.0" style="-fx-background-color: #101010;" text="X" textFill="WHITE">
      <font>
         <Font size="28.0" />
      </font>
   </Button>
   <AnchorPane fx:id="loosePane" layoutX="20.0" layoutY="20.0" prefHeight="410.0" prefWidth="614.0" style="-fx-background-color: black;">
      <children>
         <Text fill="#f8f4f4" layoutX="95.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="They have guessed your word!" textAlignment="CENTER" wrappingWidth="409.6181640625">
            <font>
               <Font name="Calibri Bold" size="56.0" />
            </font>
         </Text>
         <Button layoutX="549.0" layoutY="-4.0" mnemonicParsing="false" onAction="#loose" prefHeight="64.0" prefWidth="55.0" style="-fx-background-color: #101010;" text="X" textFill="WHITE">
            <font>
               <Font size="28.0" />
            </font>
         </Button>
         <Button contentDisplay="CENTER" layoutX="248.0" layoutY="330.0" mnemonicParsing="false" onAction="#loose" prefHeight="36.0" prefWidth="108.0" style="-fx-background-color: #353535;" text="OK" textAlignment="CENTER" textFill="#d0cece">
            <font>
               <Font name="Calibri" size="19.0" />
            </font>
         </Button>
         <Text fill="#c9c9c9" layoutX="58.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Next time try to figure out more complicated word" textAlignment="CENTER" wrappingWidth="483.84814453125" />
      </children>
   </AnchorPane>
   <AnchorPane fx:id="winPane" layoutX="20.0" layoutY="20.0" prefHeight="410.0" prefWidth="614.0"
               style="-fx-background-color: black;">
      <children>
         <Text fill="#f8f4f4" layoutX="95.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="They haven't guessed your word!" textAlignment="CENTER" wrappingWidth="409.6181640625">
            <font>
               <Font name="Calibri Bold" size="56.0" />
            </font>
         </Text>
         <Button layoutX="549.0" layoutY="-4.0" mnemonicParsing="false" onAction="#win" prefHeight="64.0"
                 prefWidth="55.0" style="-fx-background-color: #101010;" text="X" textFill="WHITE">
            <font>
               <Font size="28.0" />
            </font>
         </Button>
         <Button contentDisplay="CENTER" layoutX="248.0" layoutY="330.0" mnemonicParsing="false" onAction="#win"
                 prefHeight="36.0" prefWidth="108.0" style="-fx-background-color: #353535;" text="OK" textAlignment="CENTER" textFill="#d0cece">
            <font>
               <Font name="Calibri" size="19.0" />
            </font>
         </Button>
         <Text fill="#c9c9c9" layoutX="58.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Good work!"
               textAlignment="CENTER" wrappingWidth="483.84814453125" />
      </children>
   </AnchorPane>
    <AnchorPane fx:id="connectionErrorPane" layoutX="20.0" layoutY="20.0" prefHeight="410.0" prefWidth="614.0"
                style="-fx-background-color: black;">
        <children>
            <Text fill="#f8f4f4" layoutX="95.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Error" textAlignment="CENTER" wrappingWidth="409.6181640625">
                <font>
                    <Font name="Calibri Bold" size="56.0" />
                </font>
            </Text>
            <Button layoutX="549.0" layoutY="-4.0" mnemonicParsing="false" onAction="#error" prefHeight="64.0"
                    prefWidth="55.0" style="-fx-background-color: #101010;" text="X" textFill="WHITE">
                <font>
                    <Font size="28.0" />
                </font>
            </Button>
            <Button contentDisplay="CENTER" layoutX="248.0" layoutY="330.0" mnemonicParsing="false" onAction="#error"
                    prefHeight="36.0" prefWidth="108.0" style="-fx-background-color: #353535;" text="OK" textAlignment="CENTER" textFill="#d0cece">
                <font>
                    <Font name="Calibri" size="19.0" />
                </font>
            </Button>
            <Text fill="#c9c9c9" layoutX="58.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="Problem appeared while connecting to server :( "
                  textAlignment="CENTER" wrappingWidth="483.84814453125" />
        </children>
    </AnchorPane>
</AnchorPane>
